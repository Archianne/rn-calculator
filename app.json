{
  "expo": {
    "name": "calculator",
    "slug": "calculator",
    "version": "2.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.archianne.calculator",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2230756139193855~5158498030"
      }
    },
    "android": {
      "package": "com.archianne.calculator",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#333"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2230756139193855~5158498030"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "60b1596e-f6aa-4b30-b7f5-dba1d3e1bbea"
      }
    }
  }
}
